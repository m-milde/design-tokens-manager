{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020", "DOM"],
    "module": "ES2020",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./",
    "rootDir": "./",
    "declaration": false,
    "types": ["@figma/plugin-typings"]
  },
  "include": [
    "code.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
